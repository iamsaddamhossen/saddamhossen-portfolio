{
	"$schema": "https://schemas.wp.org/wp/6.9/theme.json",
	"version": 3,
	"settings": {
		"color": {
			"gradients": [
				{
					"gradient": "linear-gradient(135deg, #6366F1 0%, #8B5CF6 100%)",
					"name": "Indigo to Purple",
					"slug": "primary-gradient"
				},
				{
					"gradient": "linear-gradient(135deg, #667EEA 0%, #764BA2 100%)",
					"name": "Hero Gradient",
					"slug": "hero-gradient"
				},
				{
					"gradient": "linear-gradient(135deg, #EC4899 0%, #8B5CF6 100%)",
					"name": "Pink to Purple",
					"slug": "accent-gradient"
				},
				{
					"gradient": "linear-gradient(135deg, #F8FAFC 0%, #E2E8F0 100%)",
					"name": "Subtle Background",
					"slug": "subtle-gradient"
				}
			],
			"palette": [
				{
					"color": "#F8F7F5",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#191919",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#FFFFFF",
					"name": "Accent 1",
					"slug": "accent-1"
				},
				{
					"color": "#F5B684",
					"name": "Accent 2",
					"slug": "accent-2"
				},
				{
					"color": "#191919",
					"name": "Accent 3",
					"slug": "accent-3"
				},
				{
					"color": "#5F5F5F",
					"name": "Accent 4",
					"slug": "accent-4"
				},
				{
					"color": "#F1EEE9",
					"name": "Accent 5",
					"slug": "accent-5"
				},
				{
					"color": "#19191933",
					"name": "Accent 6",
					"slug": "accent-6"
				}
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1280px"
		},
		"typography": {
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
					"name": "Inter",
					"slug": "inter"
				},
				{
					"fontFamily": "'JetBrains Mono', 'Fira Code', 'Courier New', monospace",
					"name": "JetBrains Mono",
					"slug": "mono"
				},
				{
					"fontFamily": "'Manrope', sans-serif",
					"name": "Manrope",
					"slug": "manrope"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.125rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "1.5rem",
					"slug": "x-large"
				},
				{
					"name": "2X Large",
					"size": "2rem",
					"slug": "xx-large"
				},
				{
					"name": "3X Large",
					"size": "2.75rem",
					"slug": "xxx-large"
				},
				{
					"name": "Huge",
					"size": "4rem",
					"slug": "huge"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"color": "var(--wp--preset--color--contrast)"
				}
			},
			"core/post-title": {
				"color": {
					"text": "var(--wp--preset--color--accent-3)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "currentColor"
						}
					}
				}
			},
			"core/pullquote": {
				"color": {
					"text": "var(--wp--preset--color--accent-3)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "currentColor"
						}
					}
				}
			},
			"core/query-title": {
				"color": {
					"text": "var(--wp--preset--color--accent-3)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "currentColor"
						}
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--accent-4)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--accent-3)"
				}
			},
			"link": {
				"color": {
					"text": "currentColor"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.7"
		}
	}
}